{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm:dev:*\"",
    "dev:next": "next dev",
    "dev:codegen": "npm run codegen -- --watch",
    "build": "next build",
    "start": "next start",
    "pre-commit": "npm run codegen && git add **/*.generated.* **/__generated__/** && lint-staged",
    "codegen": "graphql-codegen --config codegen.yml",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx --cache --max-warnings 0"
  },
  "sideEffects": [
    "*.css"
  ],
  "dependencies": {
    "@apollo/client": "3.0.0-beta.50",
    "@apollo/link-schema": "2.0.0-beta.3",
    "@apollo/react-ssr": "4.0.0-beta.1",
    "@fortawesome/fontawesome-svg-core": "1.2.28",
    "@fortawesome/free-regular-svg-icons": "5.13.0",
    "@fortawesome/free-solid-svg-icons": "5.13.0",
    "@fortawesome/react-fontawesome": "0.1.10",
    "@widgetbot/embed-api": "1.1.3",
    "@widgetbot/react-embed": "1.2.2",
    "apollo-datasource-rest": "0.9.2",
    "apollo-server-micro": "2.14.2",
    "aws-amplify": "3.0.18",
    "cookie": "0.4.1",
    "date-fns": "2.14.0",
    "emotion": "10.0.27",
    "emotion-server": "10.0.27",
    "graphql-anywhere": "4.2.7",
    "next": "9.4.4",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "simple-oauth2": "4.1.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.15.0",
    "@graphql-codegen/near-operation-file-preset": "1.15.1",
    "@graphql-codegen/typescript": "1.15.0",
    "@graphql-codegen/typescript-operations": "1.15.0",
    "@graphql-codegen/typescript-react-apollo": "1.15.0",
    "@graphql-codegen/typescript-resolvers": "1.15.0",
    "@types/cookie": "0.4.0",
    "@types/node": "14.0.5",
    "@types/react": "16.9.38",
    "@types/simple-oauth2": "2.5.3",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "apollo-datasource": "0.7.1",
    "babel-eslint": "10.1.0",
    "babel-plugin-inline-react-svg": "1.1.1",
    "concurrently": "5.2.0",
    "eslint": "6.8.0",
    "eslint-config-react-app": "5.2.1",
    "eslint-plugin-flowtype": "4.7.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "2.5.1",
    "husky": "4.2.5",
    "lint-staged": "10.2.6",
    "prettier": "2.0.5",
    "typescript": "3.9.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix --max-warnings 0",
    "*.{js,jsx,ts,tsx,css,md,graphql}": "prettier --write"
  }
}
